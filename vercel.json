{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://dada-blog.vercel.app",
    "VERCEL": "1",
    "NODE_OPTIONS": "--max_old_space_size=4096",
    "NEXT_PUBLIC_USE_FALLBACK_DATA": "true",
    "TURSO_DATABASE_URL": "",
    "TURSO_AUTH_TOKEN": ""
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_SITE_URL": "https://dada-blog.vercel.app",
      "VERCEL": "1",
      "NODE_OPTIONS": "--max_old_space_size=4096",
      "NEXT_PUBLIC_USE_FALLBACK_DATA": "true",
      "TURSO_DATABASE_URL": "",
      "TURSO_AUTH_TOKEN": ""
    }
  }
} 